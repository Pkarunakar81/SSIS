                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select *
from (
SELECT case 
when TABLE_NAME in ('CBS_HTD','CBS_HTH') OR SOURCE_SYSTEM='CBS' then 'CBS_DAILY' 
when TABLE_NAME in ('CRM_CLIENT') OR SOURCE_SYSTEM='CRM' then 'CRM_DAILY' 
when SOURCE_SYSTEM='BR.NET' then 'BRNET_DAILY' 
when SOURCE_SYSTEM='MIS' then 'DAILY_MIS' 
end PackageName
,count(distinct TABLE_NAME)Total_Tables,count(distinct TaskName)Loaded,count(distinct TABLE_NAME)-count(distinct TaskName) Yet_to_oad
FROM INTDATA.ETL_TABLE_DETAILS A
LEFT JOIN  (SELECT * FROM Audit_ETLPkg 
WHERE convert(date,LoadEndTime)=convert(date,getdate()))  B ON A.TABLE_NAME=B.TaskName
where FREQUENCY='DAILY' --and B.TaskName Is not  NULL 
group by case 
when TABLE_NAME in ('CBS_HTD','CBS_HTH') OR SOURCE_SYSTEM='CBS' then 'CBS_DAILY' 
when TABLE_NAME in ('CRM_CLIENT') OR SOURCE_SYSTEM='CRM' then 'CRM_DAILY' 
when SOURCE_SYSTEM='BR.NET' then 'BRNET_DAILY' 
when SOURCE_SYSTEM='MIS' then 'DAILY_MIS' 
end  
)a where Total_Tables&lt;&gt;Loaded
--order by P</property>
