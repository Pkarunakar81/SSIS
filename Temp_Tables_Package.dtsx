                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  DISTINCT sc.name + '.' + t.NAME AS TableName,  
        p.[Rows]
		--,( SUM(a.total_pages) * 8 ) / 1024 AS TotalReservedSpaceMB
       -- ,( SUM(a.total_pages) * 8 ) / 1024/ 1024  AS TotalReservedSpaceGB
		,(SUM(a.used_pages) * 8 ) / 1024 AS UsedDataSpaceMB  
		,(SUM(a.used_pages) * 8 ) / 1024/ 1024  AS UsedDataSpaceGB  
       -- ,( SUM(a.data_pages) * 8 ) / 1024 AS FreeUnusedSpaceMB  
       -- ,( SUM(a.data_pages) * 8 ) / 1024/ 1024  AS FreeUnusedSpaceGB  
FROM    sys.tables t  
        INNER JOIN sys.schemas sc ON sc.schema_id = t.schema_id  
        INNER JOIN sys.indexes i ON t.OBJECT_ID = i.object_id  
        INNER JOIN sys.partitions p ON i.object_id = p.OBJECT_ID  
                                            AND i.index_id = p.index_id  
        INNER JOIN sys.allocation_units a ON p.partition_id = a.container_id  
WHERE   t.type_desc = 'USER_TABLE'  
        AND i.index_id &lt;= 1  --- Heap\ CLUSTERED
        --AND t.NAME='REQ_CKYC_KAR' -- Replace with valid table name
		and sc.name='dbo' 
		AND t.NAME NOT LIKE '%2001%'
		AND t.NAME NOT LIKE 't_rpt_dailyReceivables_Vs_Collections_Monthly%'
		AND t.NAME NOT LIKE 'T_ACCOUNTCUSTOMER%'
		AND t.NAME NOT LIKE 'MICROBANKING_DUE%'
		AND t.NAME NOT LIKE 'Credit_Repayment%'
GROUP BY sc.name + '.' + t.NAME,  
        i.[object_id],i.index_id, i.name, p.[Rows]  
ORDER BY (SUM(a.used_pages) * 8 ) / 1024 DESC</property>
